{
  "name": "cucumber-puppeteer",
  "version": "0.0.1",
  "description": "Boilerplate for developing an integration testing suite using Cucumber and Puppeteer.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"{,!(node_modules)/**/}*.{js}\"",
    "cucumber": "./node_modules/.bin/cucumber-js",
    "debug": "node --inspect=1337 --debug-brk --nolazy node_modules/cucumber/bin/cucumber-js --tags @debug --format json:./reports/report.json",
    "test:alpha": "NODE_ENV=alpha ./node_modules/.bin/cucumber-js  --format json:./reports/report.json --fail-fast",
    "test:debug": "NODE_ENV=alpha DEBUG=true CLIENT_ENV=okdoc-nfcc cucumber-js features --format json:./reports/report.json --tags 'not @ignore' --fail-fast",
    "test:dev": "NODE_ENV=dev ./node_modules/.bin/cucumber-js  --format json:./reports/report.json --fail-fast",
    "test:prod": "NODE_ENV=prod ./node_modules/.bin/cucumber-js  --format json:./reports/report.json --fail-fast"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlampedx/cucumber-puppeteer.git"
  },
  "keywords": [
    "Cucumber",
    "Puppeteer",
    "integration",
    "testing",
    "JavaScript",
    "Chrome"
  ],
  "author": "Michael Lampe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlampedx/cucumber-puppeteer/issues"
  },
  "homepage": "https://github.com/mlampedx/cucumber-puppeteer#readme",
  "devDependencies": {
    "cucumber": "^5.1.0",
    "puppeteer": "^4.0.1"
  },
  "dependencies": {
    "data.task": "^3.1.2",
    "immutable": "^4.0.0-rc.12",
    "immutable-ext": "^1.1.5",
    "or": "^0.2.0"
  }
}
